#include <iostream> 
using namespace std; 

int main(){ 
  int y1,y2,n; 

  for(n=0;n<=100;n+=10){ 
    y1=100*n+10; 
    y2=5*n*n+2; 
    if(y1==y2) 
      cout<<"y1 and y2 are equal  n = "<<n<<endl; 
    else if(y1>y2) 
      cout<<"y1 wins  n = "<<n<<endl; 
    else if(y1<y2) 
      cout<<"y2 wins  n = " <<n<<endl; 

   } 

  return 0; 

} 
